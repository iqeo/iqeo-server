<h1>Listing ip4_networks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th>Mask</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ip4_networks.each do |ip4_network| %>
      <tr>
        <td><%= ip4_network.name %></td>
        <td><%= ip4_network.desc %></td>
        <td><%= ip4_network.mask %></td>
        <td><%= link_to 'Show', ip4_network %></td>
        <td><%= link_to 'Edit', edit_ip4_network_path(ip4_network) %></td>
        <td><%= link_to 'Destroy', ip4_network, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ip4 network', new_ip4_network_path %>
